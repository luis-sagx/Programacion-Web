@{
    ViewData["Title"] = "Index";
}

<h1>Laboratorio</h1>

<fieldset>
    <form id="formBusqueda" method="post" class="row g-3 m-3 p-3" style="border: 1px solid blue; border-radius:10px">
        <div class="col-md-6">
            <label for="nombre" class="form-label">Nombre</label>
            <input type="text" class="form-control" id="inputNombre" name="nombre">
        </div>
        <div class="col-md-6">
            <label for="direccion" class="form-label">Direccion</label>
            <input type="text" class="form-control" id="inputDireccion" name="direccion">
        </div>
        <div class="col-12">
            <label for="personaContacto" class="form-label">Persona de contacto</label>
            <input type="text" class="form-control" id="inputPersonaContacto" name="personaContacto">
        </div>
        <div class="col-12">
            <button type="button" class="btn btn-primary" onclick="buscarLaboratorio()">Buscar</button>
        </div>
        <div class="col-12">
            <button type="button" onclick="limpiarLaboratorio()" class="btn btn-secondary">Borrar</button>
        </div>
    </form>
</fieldset>

<div id="divTable">
</div>

<script src="~/js/generico.js"></script>
<script src="~/js/laboratorio.js"></script>
